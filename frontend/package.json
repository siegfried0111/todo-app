    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.13.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.65.0",
    "react-router-dom": "^7.9.5",
    "zod": "^4.1.12"
